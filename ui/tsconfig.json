{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"verbatimModuleSyntax": true,
		"isolatedModules": true,
		"lib": ["esnext", "DOM", "DOM.Iterable"],
		"noEmit": true
	}
}
